{
	"sites": {
		"docs": {
			"dest": "docs",
			"parent": "FuncUnit",
			"pageConfig": {"page":"docs"},
			"glob": {
				"pattern": "{browser/**/*.{js,md},README.md}"
			}
		},
		"guides":{
			"dest": "guides",
			"glob": {
				"pattern": "doc/guides/**/*.{js,md}"
			},
			"parent": "Guides",
			"pageConfig": {"page":"guides"}
		},
		"funcunit": {
			"glob": {
				"pattern": "funcunit.md"
			},
			"templates": "main"
		}
	},
	"siteDefaults": {
		"static":"site/theme/static",
		"templates": "site/theme/templates"
	}
}
